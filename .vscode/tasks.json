{
    "version": "2.0.0",
    "command": "g++",
    "isShellCommand": true,
    "showOutput": "always",
    "args": [
        "-o",
        "${fileBasenameNoExtension}",
        "${file}",
        "&",
        "./${fileBasenameNoExtension}"
    ],
    "isBuildCommand": true,
    "problemMatchers": [
        "$gcc"
    ]
}
